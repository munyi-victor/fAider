{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6"],
    "sourceMap": true,
    "outDir": "dist",
    "module": "NodeNext",
    "rootDir": "src",
    "resolveJsonModule": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "lib": ["dom", "dom.iterable", "es2015.promise"]
}
